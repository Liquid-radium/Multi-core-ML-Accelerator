# vsim tb_gpio 
# Start time: 18:21:46 on Jul 22,2025
# Loading sv_std.std
# Loading work.tb_gpio
# Loading work.gpio_module
force -deposit /tb_gpio/clk 0
run 5ns
force -deposit /tb_gpio/clk 1
run 5ns

restart -f
add wave sim:/tb_gpio/*
run 100ns

force -deposit /tb_gpio/clk 0
run 5ns
force -deposit /tb_gpio/clk 1
run 5ns

vlib work
# ** Warning: (vlib-34) Library already exists at "work".
vlog tb_gpio.v gpio_module.v
